<td class="event-thumb">
  <% if (photo_content_type) { %>
    <img src="<%= photo_url_small %>" width=100></img>
  <% } else if (facebook_id) { %>
    <img src="https://graph.facebook.com/<%= facebook_id %>/picture?type=normal" width=100></img>
  <% } else { %>
    <!-- [TODO] use .erb and image_tag somehow -->
    <img src="http://eventsalsa.com/images/favicon.ico" width=100></img>
  <% } %>
</td>

<td class="event-console">
  <% if (can_manage) { %>
    <a class="edit">Edit</a>
    <a class="delete">Delete</a>
  <% } %>

  <% if (start_time) { %>
    <h5 class="humane-time">
      <%= moment(start_time).from(moment()) %>
    </h5>
    <h6 class="date-time">
      <%= moment(start_time).format('M/D/YY h:mmA') %>
    </h6>
  <% } %>
  <div class="attending-control"></div>
  <% if (false) { %>
    <a href="#">Edit Event</a>
  <% } %>
</td>

<td class="event-detail">
</td>

<div class="navbar navbar-fixed-top">
  <div class="navbar-inner">
    <div class="container">
      <a class="brand" href="#">EventSalsa</a>
      <a id="fblike"><fb:like href="https://www.facebook.com/pages/EventSalsa/165333256908690" send="false" width="80" layout="button_count" show_faces="false" action="like" font="" allowTransparency="true"></fb:like></a>
      <ul id="navbar" class="nav">
        <li>
          <a data-target="calendar">Calendar</a>
        </li>
        <li class="active">
          <a>Events</a>
        </li>
        <li>
          <a>Users</a>
        </li>
        <li>
          <a>Profile</a>
        </li>

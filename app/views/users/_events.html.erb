<script type="text/javascript">
	$(document).ready(function(){
		$('a.delete').click(function(){
			if(confirm("Are you sure you want to delete this event?")){
				$.post(this.href, { _method: 'delete' }, null, "script");
				
				return false;
			}
		});
	});
</script>

